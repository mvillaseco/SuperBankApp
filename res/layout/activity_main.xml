<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    android:layout_margin="0dp"
    android:layout_weight="1.0"
    android:background="@color/negro"
    android:gravity="center"
    tools:context=".MainActivity" >

 <TextView
        android:id="@+id/textView1"
        android:layout_gravity="top"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="30dp"
        android:textColor="@color/blancoReal"
        style="@style/ButtonText"
        android:text="@string/preguntaDepositar" />
   
             <Button
                 android:id="@+id/uno"                 
        		 style="@style/ButtonText"
                 android:layout_width="@dimen/buttonWidth"
                 android:layout_height="@dimen/buttonHeight"
                 android:layout_below="@+id/textView1"
                 android:layout_marginRight="@dimen/margenRight"
                 android:layout_marginTop="@dimen/margenTop"
                 android:onClick="OnClick"
                 android:background="@drawable/buttonsdrawable"
                 android:textColor="@color/blancoReal"
                 android:tag="@string/uno"
                 android:text="@string/uno" />

             <Button
                 android:id="@+id/dos"
                 android:layout_width="@dimen/buttonWidth"
                 android:layout_height="@dimen/buttonHeight"
                 android:layout_below="@+id/textView1"
                 android:layout_marginRight="@dimen/margenRight"
                 android:layout_marginTop="@dimen/margenTop"
                 android:layout_toRightOf="@+id/uno"
                 android:onClick="OnClick"
                 android:textColor="@color/blancoReal"
                 android:background="@drawable/buttonsdrawable"
                 android:tag="@string/dos"
                 style="@style/ButtonText"
                 android:text="@string/dos" />
             
             <Button
                 android:id="@+id/tres"
                 android:layout_width="@dimen/buttonWidth"
                 android:layout_height="@dimen/buttonHeight"
                 android:layout_toRightOf="@+id/dos"
                 android:layout_marginTop="@dimen/margenTop"                 
                 android:layout_below="@+id/textView1"
                 android:tag="@string/tres"
                 android:textColor="@color/blancoReal"
                 android:onClick="OnClick"
                 style="@style/ButtonText"
                 android:background="@drawable/buttonsdrawable"
                 android:text="@string/tres" />



              <Button
                 android:id="@+id/cuatro"
                 android:layout_width="@dimen/buttonWidth"
                 android:layout_height="@dimen/buttonHeight"
                 android:layout_below="@+id/uno"
                 android:layout_marginRight="@dimen/margenRight"
                 android:layout_marginTop="@dimen/margenTop"
                 android:tag="@string/cuatro"
                 android:onClick="OnClick"
                 android:textColor="@color/blancoReal"
                 style="@style/ButtonText"
                 android:background="@drawable/buttonsdrawable"
                 android:text="@string/cuatro" />
              
              <Button
                 android:id="@+id/cinco"
                 android:layout_width="@dimen/buttonWidth"
                 android:layout_height="@dimen/buttonHeight"
                 android:layout_below="@+id/dos"
                 android:layout_toRightOf="@+id/cuatro"
                 android:layout_marginRight="@dimen/margenRight"
                 style="@style/ButtonText"
                 android:layout_marginTop="@dimen/margenTop"
                 android:tag="@string/cinco"
                 android:textColor="@color/blancoReal"
                 android:onClick="OnClick"
                 android:background="@drawable/buttonsdrawable"
                 android:text="@string/cinco" />

             <Button
                 android:id="@+id/seis"
                 android:layout_width="@dimen/buttonWidth"
                 android:layout_height="@dimen/buttonHeight"
                 android:layout_marginTop="@dimen/margenTop"
                 android:layout_below="@+id/tres"
                 android:layout_toRightOf="@+id/cinco"
                 android:textColor="@color/blancoReal"
                 style="@style/ButtonText"
                 android:tag="@string/seis"
                 android:background="@drawable/buttonsdrawable"
                 android:onClick="OnClick"
                 android:text="@string/seis" />
        
             
             <Button
                 android:id="@+id/siete"
                 android:layout_width="@dimen/buttonWidth"
                 android:layout_height="@dimen/buttonHeight"
                 android:layout_marginTop="@dimen/margenTop"
                 android:layout_below="@+id/cuatro"
                  android:layout_marginRight="@dimen/margenRight"
                  android:tag="@string/siete"
                  style="@style/ButtonText"
                  android:textColor="@color/blancoReal"
                  android:onClick="OnClick"
                  android:background="@drawable/buttonsdrawable"
                 android:text="@string/siete" />

			<Button
                 android:id="@+id/ocho"
                 android:layout_width="@dimen/buttonWidth"
                 android:layout_height="@dimen/buttonHeight"
                 android:layout_marginTop="@dimen/margenTop"
                 android:textColor="@color/blancoReal"
                 android:layout_below="@+id/cinco"
                 style="@style/ButtonText"
                 android:layout_toRightOf="@+id/siete"
                 android:tag="@string/ocho"
                 android:onClick="OnClick"
                 android:background="@drawable/buttonsdrawable"
                 android:layout_marginRight="@dimen/margenRight"
                 android:text="@string/ocho" />

             
             <Button
                 android:id="@+id/nueve"
                 android:layout_width="@dimen/buttonWidth"
                 android:layout_height="@dimen/buttonHeight"
                 android:layout_marginTop="@dimen/margenTop"
                 android:textColor="@color/blancoReal"
                 android:layout_below="@+id/seis"
                 style="@style/ButtonText"
                 android:layout_toRightOf="@+id/ocho"
                 android:tag="@string/nueve"
                 android:onClick="OnClick"
                 android:background="@drawable/buttonsdrawable"
                 android:text="@string/nueve" />

             <Button
                 android:id="@+id/diez"
                 android:layout_width="@dimen/buttonWidth"
                 android:layout_height="@dimen/buttonHeight"
                 android:layout_marginTop="@dimen/margenTop"
                 android:layout_below="@+id/siete"
                 android:tag="1000"
                 android:textColor="@color/blancoReal"
                 style="@style/ButtonText"
                 android:onClick="OnClick"
                 android:background="@drawable/buttonsdrawable"
                 android:layout_marginRight="@dimen/margenRight"
                 android:text="@string/mil" />
             
             <Button
                 android:id="@+id/quince"
                 android:layout_width="@dimen/buttonWidth"
                 android:layout_height="@dimen/buttonHeight"
                 android:layout_marginRight="@dimen/margenRight"
                 android:layout_marginTop="@dimen/margenTop"
                 android:layout_below="@+id/ocho"
                 android:textColor="@color/blancoReal"
                 android:layout_toRightOf="@+id/diez"
                 style="@style/ButtonText"
                 android:tag="3000"
                 android:onClick="OnClick"
                 android:background="@drawable/buttonsdrawable"
                 android:text="@string/tresmil" />

			<Button
                 android:id="@+id/veinte"
                 android:layout_width="@dimen/buttonWidth"
                 android:layout_height="@dimen/buttonHeight"
                 android:layout_marginTop="@dimen/margenTop"
                 android:textColor="@color/blancoReal"
                 android:layout_below="@+id/nueve"
                 style="@style/ButtonText"
                 android:tag="5000"
                 android:onClick="OnClick"
                 android:background="@drawable/buttonsdrawable"
                 android:layout_toRightOf="@+id/quince"
                 android:text="@string/cincomil" />

			<ToggleButton
			    android:id="@+id/toggleButton"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    style="@style/ButtonText"
			    android:layout_alignRight="@+id/tres"
			    android:layout_alignTop="@+id/textView1"
			    android:textOff="@string/ToggleDescontar"
			    android:textOn="@string/ToggleDepositar"
			    android:textColor="@color/blancoReal"
			    android:onClick="ToggleChanged" />

</RelativeLayout>